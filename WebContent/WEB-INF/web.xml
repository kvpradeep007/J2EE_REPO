<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>My_J2EE_Project</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>login.html</welcome-file>
    <welcome-file>login.htm</welcome-file>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
  
   <servlet>
    <servlet-name>HelloGenericServlet</servlet-name>
    <servlet-class>com.j2ee.servlet.HelloGenericServlet</servlet-class>
   </servlet>
   
  <servlet-mapping>
      <servlet-name>HelloGenericServlet</servlet-name>
      <url-pattern>/FirstHelloGenericWorld</url-pattern>
  </servlet-mapping>
    
  <servlet>
    <servlet-name>HelloHttpServlet</servlet-name>
    <servlet-class>com.j2ee.servlet.HelloHttpServlet</servlet-class>
   </servlet>
     
  <servlet-mapping>
      <servlet-name>HelloHttpServlet</servlet-name>
      <url-pattern>/FirstHelloHttpWorld</url-pattern>
  </servlet-mapping>
    
  <servlet>
      <servlet-name>sumOfTwoNumbers</servlet-name>
      <servlet-class>com.j2ee.servlet.GetParameter</servlet-class>
  </servlet>
 
  <servlet-mapping>
      <servlet-name>sumOfTwoNumbers</servlet-name>
      <url-pattern>/sum</url-pattern>
  </servlet-mapping>
   
 <servlet>
   <servlet-name>loginServlet</servlet-name>
   <servlet-class>com.j2ee.servlet.LoginServlet</servlet-class>
 </servlet>
 
 <servlet-mapping>
   <servlet-name>loginServlet</servlet-name>
   <url-pattern>/login</url-pattern>
 </servlet-mapping>
 
 <welcome-file-list>
   <welcome-file>login.html</welcome-file>
 </welcome-file-list>
</web-app>